#include <stdio.h>

void factores_primos(int n) {
   int i;
   for (i = 2; i <= n; i++) {
      while (n % i == 0) {
         printf("%d ", i);
         n = n / i;
      }
   }
}

int main() {
   int num;
   printf("Introduce un nÃºmero: ");
   scanf("%d", &num);
   printf("Factores primos: ");
   factores_primos(num);
   return 0;
}
